{
  "name": "avox",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "assemble:staging": "cd android && EXPO_PUBLIC_ENV=staging ./gradlew assembleStagingRelease && cd ..",
    "assemble:production": "cd android && EXPO_PUBLIC_ENV=production ./gradlew assembleProductionRelease && cd ..",
    "bundle:staging": "cd android && EXPO_PUBLIC_ENV=staging ./gradlew bundleStagingRelease && cd ..",
    "bundle:production": "cd android && EXPO_PUBLIC_ENV=production ./gradlew bundleProductionRelease && cd ..",
    "android:staging": "EXPO_PUBLIC_ENV=staging expo run:android --variant=stagingDebug",
    "android:staging:release": "EXPO_PUBLIC_ENV=staging expo run:android --variant=stagingRelease",
    "android:production": "EXPO_PUBLIC_ENV=production expo run:android --variant=productionDebug",
    "android:production:release": "EXPO_PUBLIC_ENV=production expo run:android --variant=productionRelease",
    "ios:staging": "EXPO_PUBLIC_ENV=staging expo run:ios --scheme 'Avox Staging'",
    "ios:staging:release": "EXPO_PUBLIC_ENV=staging expo run:ios --scheme 'Avox Staging' --configuration Release",
    "ios:production": "EXPO_PUBLIC_ENV=production expo run:ios --scheme Avox",
    "ios:production:release": "EXPO_PUBLIC_ENV=production expo run:ios --scheme Avox --configuration Release",
    "lint": "expo lint",
    "postinstall": "patch-package",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write .",
    "format:lint": "prettier --write . && expo lint --fix",
    "start:staging": "EXPO_PUBLIC_ENV=staging expo start --clear",
    "start:production": "EXPO_PUBLIC_ENV=production expo start --clear",
    "build:staging": "EXPO_PUBLIC_ENV=staging eas build --profile staging --platform all",
    "build:staging:android": "EXPO_PUBLIC_ENV=staging eas build --profile staging --platform android",
    "build:staging:ios": "EXPO_PUBLIC_ENV=staging eas build --profile staging --platform ios",
    "build:production": "EXPO_PUBLIC_ENV=production eas build --profile production --platform all",
    "build:production:android": "EXPO_PUBLIC_ENV=production eas build --profile production --platform android",
    "build:production:ios": "EXPO_PUBLIC_ENV=production eas build --profile production --platform ios",
    "submit:staging:android": "eas submit --profile staging --platform android",
    "submit:staging:ios": "eas submit --profile staging --platform ios",
    "submit:production:android": "eas submit --profile production --platform android",
    "submit:production:ios": "eas submit --profile production --platform ios",
    "update:staging": "EXPO_PUBLIC_ENV=staging eas update --branch staging",
    "update:production": "EXPO_PUBLIC_ENV=production eas update --branch production",
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update-snapshots": "find . -name '*.snap' -delete && jest --updateSnapshot",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:maestro": "maestro test e2e/maestro"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^15.0.2",
